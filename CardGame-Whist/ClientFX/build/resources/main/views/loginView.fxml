<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="301.0" prefWidth="333.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.LoginController">
  <children>
    <Label layoutX="89.0" layoutY="39.0" text="WHIST GAME">
      <font>
        <Font size="26.0" />
      </font>
    </Label>
    <Label layoutX="108.0" layoutY="94.0" text="Enter nickname">
      <font>
        <Font size="17.0" />
      </font>
    </Label>
    <TextField fx:id="nicknameTxt" alignment="CENTER" layoutX="88.0" layoutY="120.0" promptText="guest" />
    <Button layoutX="109.0" layoutY="266.0" mnemonicParsing="false" onAction="#login" prefHeight="26.0" prefWidth="116.0" text="Join" />
    <Label layoutX="109.0" layoutY="146.0" text="Server IP Address" />
    <TextField id="nicknameTxt" fx:id="ipAddr" alignment="CENTER" layoutX="88.0" layoutY="166.0" promptText="127.0.0.1" />
    <Label layoutX="150.0" layoutY="192.0" text="Port" />
    <TextField id="nicknameTxt" fx:id="port" alignment="CENTER" layoutX="88.0" layoutY="212.0" promptText="1099" />
    <HBox alignment="CENTER" layoutX="1.0" layoutY="238.0" prefHeight="27.0" prefWidth="333.0">
      <children>
        <Label fx:id="errorMessage" text="This nickname is already used" textFill="RED" />
      </children>
    </HBox>
  </children>
</AnchorPane>
